{
	"name": "logos-link",
	"version": "1.1.3",
	"description": "Make linking to Logos Bible Software easy.",
	"author": "David Troyer",
	"license": "MIT",
	"keywords": [
		"Logos",
		"Link",
		"Bible",
		"Passage"
	],
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "jest",
		"test:watch": "jest --watch"
	},
	"prettier": {
		"printWidth": 100,
		"singleQuote": true
	},
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.25.0",
		"jest": "^29.7.0",
		"obsidian": "latest",
		"prettier": "^3.7.3",
		"ts-jest": "^29.4.5",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
